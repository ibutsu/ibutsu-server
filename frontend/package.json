{
  "name": "ibutsu-frontend",
  "version": "2.0.2",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/helper-call-delegate": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@babel/preset-flow": "^7.14.5",
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.10",
    "@greatsumini/react-facebook-login": "^2.1.5",
    "@hapi/hoek": "^6.2.4",
    "@hapi/joi": "^15.0.0",
    "@hapi/topo": "^3.1.0",
    "@monaco-editor/react": "3.2.1",
    "@patternfly/patternfly": "^4.10.31",
    "@patternfly/react-charts": "^6.3.9",
    "@patternfly/react-core": "^4.18.5",
    "@patternfly/react-icons": "^4.3.5",
    "@patternfly/react-styles": "^4.11.0",
    "@patternfly/react-table": "^4.5.7",
    "@patternfly/react-tokens": "^4.12.0",
    "@react-keycloak/web": "^3.4.0",
    "acorn": "^8.4.1",
    "core-js": "^3.1.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.30.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.19.0",
    "history": "^5.0.0",
    "js-levenshtein": "^1.1.6",
    "keycloak-js": "^14.0.0",
    "monaco-editor": "^0.25.2",
    "prop-types": "^15.7.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-facebook-login": "^4.1.1",
    "react-github-login": "^1.0.3",
    "react-google-login": "^5.2.2",
    "react-heatmap-grid": "^0.8.1",
    "react-json-view": "^1.19.1",
    "react-linkify": "^1.0.0-alpha",
    "react-password-strength-bar": "^0.3.5",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^4.0.3",
    "react-simple-jstree": "^0.2.2",
    "react-simple-oauth2-login": "^0.5.0",
    "react-test-renderer": "^16.0.0",
    "react-typeahead": "^2.0.0-alpha.8",
    "restful.js": "^0.9.6",
    "serve": "^12.0.0",
    "typescript": "^4.3.5",
    "wolfy87-eventemitter": "^5.2.9",
    "yarn": "^1.22.0"
  },
  "scripts": {
    "start": "serve -s build -l tcp://0.0.0.0:8080",
    "build": "./bin/write-version-file.js && react-scripts build",
    "test": "./bin/write-version-file.js && react-scripts test",
    "eject": "react-scripts eject",
    "devserver": "./bin/write-version-file.js && react-scripts start",
    "devserver-ssl": "./bin/write-version-file.js && HTTPS=true SSL_CRT_FILE=../certs/dev.ibutsu.org+2.pem SSL_KEY_FILE=../certs/dev.ibutsu.org+2-key.pem react-scripts start",
    "lint": "eslint --ext=js --ext=jsx ./src"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2017": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "sourceType": "module",
      "requireConfigFile": false,
      "babelOptions": {
        "plugins": [
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-proposal-private-methods",
          "@babel/plugin-syntax-jsx"
        ],
        "presets": [
          "@babel/preset-flow"
        ]
      }
    },
    "plugins": [
      "react",
      "import"
    ],
    "settings": {
      "react": {
        "version": "16.0"
      },
      "propWrapperFunctions": [
        "forbidExtraProps",
        {
          "property": "freeze",
          "object": "Object"
        },
        {
          "property": "myFavoriteWrapper"
        }
      ],
      "linkComponents": [
        "Hyperlink",
        {
          "name": "Link",
          "linkAttribute": "to"
        }
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "bin": {
    "write-version-file": "./bin/write-version-file.js"
  }
}

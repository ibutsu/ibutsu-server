{
  "name": "ibutsu-frontend",
  "version": "2.4.2",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/helper-call-delegate": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/preset-flow": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@greatsumini/react-facebook-login": "^2.1.5",
    "@monaco-editor/react": "^4.4.6",
    "@patternfly/patternfly": "^4.215.1",
    "@patternfly/react-charts": "^6.92.0",
    "@patternfly/react-core": "^4.181.1",
    "@patternfly/react-icons": "^4.32.1",
    "@patternfly/react-styles": "^4.31.1",
    "@patternfly/react-table": "^4.108.0",
    "@react-keycloak/web": "^3.4.0",
    "@testing-library/react": "^12.1.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "eslint": "^7.30.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.19.0",
    "keycloak-js": "^19.0.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-google-login": "^5.2.2",
    "react-heatmap-grid": "^0.9.0",
    "react-json-view": "^1.19.1",
    "react-linkify": "^1.0.0-alpha",
    "react-password-strength-bar": "^0.3.5",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^4.0.3",
    "react-simple-jstree": "^0.2.2",
    "react-simple-oauth2-login": "^0.5.0",
    "serve": "^12.0.0",
    "typescript": "^4.3.5",
    "wolfy87-eventemitter": "^5.2.9"
  },
  "scripts": {
    "start": "serve -s build -l tcp://0.0.0.0:8080",
    "build": "./bin/write-version-file.js && react-scripts build",
    "test": "./bin/write-version-file.js && react-scripts test",
    "eject": "react-scripts eject",
    "devserver": "./bin/write-version-file.js && CI=true react-scripts start",
    "lint": "eslint --ext=js --ext=jsx ./src"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2017": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "sourceType": "module",
      "requireConfigFile": false,
      "babelOptions": {
        "plugins": [
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-proposal-private-methods",
          "@babel/plugin-syntax-jsx"
        ],
        "presets": [
          "@babel/preset-flow"
        ]
      }
    },
    "plugins": [
      "react",
      "import"
    ],
    "settings": {
      "react": {
        "version": "16.0"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bin": {
    "write-version-file": "./bin/write-version-file.js"
  }
}

{
  "name": "ibutsu-frontend",
  "version": "1.11.2",
  "private": true,
  "dependencies": {
    "@babel/helper-call-delegate": "^7.8.7",
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.10",
    "@hapi/hoek": "^6.2.4",
    "@hapi/joi": "^15.0.0",
    "@hapi/topo": "^3.1.0",
    "@monaco-editor/react": "3.2.1",
    "@patternfly/patternfly": "^4.10.31",
    "@patternfly/react-charts": "^6.3.9",
    "@patternfly/react-core": "^4.18.5",
    "@patternfly/react-icons": "^4.3.5",
    "@patternfly/react-styles": "^3.0.2",
    "@patternfly/react-table": "^4.5.7",
    "@patternfly/react-tokens": "^2.3.3",
    "acorn": "^6.1.1",
    "core-js": "^3.1.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.19.0",
    "js-levenshtein": "^1.1.6",
    "monaco-editor": "^0.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.8.6",
    "react-heatmap-grid": "^0.8.1",
    "react-json-view": "^1.19.1",
    "react-linkify": "^1.0.0-alpha",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.4.1",
    "react-simple-jstree": "^0.2.2",
    "react-test-renderer": "^16.13.1",
    "react-typeahead": "^2.0.0-alpha.8",
    "restful.js": "^0.9.6",
    "serve": "^11.0.1",
    "typescript": "^3.5.1",
    "wolfy87-eventemitter": "^5.2.9",
    "yarn": "^1.22.0"
  },
  "scripts": {
    "start": "serve -s build -l tcp://0.0.0.0:8080",
    "build": "./bin/write-version-file.js && react-scripts build",
    "test": "./bin/write-version-file.js && react-scripts test",
    "eject": "react-scripts eject",
    "devserver": "./bin/write-version-file.js && react-scripts start",
    "lint": "eslint --ext=js --ext=jsx ./src"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2017": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "import"
    ],
    "settings": {
      "react": {
        "version": "detect"
      },
      "propWrapperFunctions": [
        "forbidExtraProps",
        {
          "property": "freeze",
          "object": "Object"
        },
        {
          "property": "myFavoriteWrapper"
        }
      ],
      "linkComponents": [
        "Hyperlink",
        {
          "name": "Link",
          "linkAttribute": "to"
        }
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "bin": {
    "write-version-file": "./bin/write-version-file.js"
  }
}

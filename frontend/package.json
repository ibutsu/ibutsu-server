{
  "name": "ibutsu-frontend",
  "version": "3.0.1",
  "private": true,
  "description": "Ibutsu Frontend",
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@greatsumini/react-facebook-login": "^3.3.3",
    "@patternfly/patternfly": "^6.4.0",
    "@patternfly/react-charts": "^8.4.0",
    "@patternfly/react-code-editor": "^6.4.0",
    "@patternfly/react-component-groups": "^6.4.0",
    "@patternfly/react-core": "^6.4.0",
    "@patternfly/react-drag-drop": "^6.4.0",
    "@patternfly/react-icons": "^6.4.0",
    "@patternfly/react-log-viewer": "^6.3.0",
    "@patternfly/react-styles": "^6.4.0",
    "@patternfly/react-table": "^6.4.0",
    "@react-oauth/google": "^0.12.1",
    "@types/react": "^18.0.0",
    "apexcharts": "^4.7.0",
    "enzyme": "^3.11.0",
    "eslint-config": "^0.3.0",
    "fs-extra": "^11.3.2",
    "globals": "^16.5.0",
    "keycloak-js": "^19.0.3",
    "monaco-editor": "^0.47.0",
    "nanoid": "^5.1.6",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-apexcharts": "^1.9.0",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-heatmap-grid": "^0.9.0",
    "react-linkify": "^1.0.0-alpha",
    "react-password-strength-bar": "^0.4.1",
    "react-router-dom": "^6.28.0",
    "react-scripts": "^5.0.1",
    "react-simple-oauth2-login": "^0.5.4",
    "react-toastify": "^11.0.2",
    "serve": "^14.2.5",
    "typescript": "^4.9.5",
    "victory": "^37.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/helper-call-delegate": "^7.12.13",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-syntax-jsx": "^7.24.7",
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/plugin-transform-private-methods": "^7.24.7",
    "@babel/plugin-transform-private-property-in-object": "^7.24.7",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-flow": "^7.24.7",
    "@babel/preset-react": "^7.28.5",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "babel-jest": "^30.2.0",
    "cypress": "^14.4.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "prettier": "^3.7.4"
  },
  "scripts": {
    "start": "serve -s build -l tcp://0.0.0.0:8080",
    "build": "./bin/write-version-file.cjs && DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "test": "./bin/write-version-file.cjs && react-scripts test --watchAll=false --transformIgnorePatterns \"node_modules/(?!@patternfly)/\"",
    "test:coverage": "./bin/write-version-file.cjs && react-scripts test --watchAll=false --coverage --silent --transformIgnorePatterns \"node_modules/(?!@patternfly)/\"",
    "test:coverage:ci": "./bin/write-version-file.cjs && react-scripts test --watchAll=false --coverage --silent --ci --maxWorkers=2 --transformIgnorePatterns \"node_modules/(?!@patternfly)/\"",
    "eject": "react-scripts eject",
    "devserver": "./bin/write-version-file.cjs && PORT=3000 CI=true DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "lint": "eslint --fix",
    "format": "prettier \"src/**/*.{js,html}\" --write",
    "format-check": "prettier \"src/**/*.{js,html}\" --check"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all",
      "not ie <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ]
  },
  "bin": {
    "write-version-file": "./bin/write-version-file.cjs"
  },
  "jest": {
    "resetMocks": false,
    "transformIgnorePatterns": [
      "node_modules/(?!(@patternfly|@testing-library)/)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/setupTests.js",
      "!src/**/*.test.js",
      "!src/**/*.spec.js",
      "!src/jest.polyfills.cjs",
      "!src/views/accessibility-*.js",
      "!src/test-utils/**"
    ],
    "coverageReporters": [
      "text-summary",
      "html",
      "lcov",
      "json-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 28,
        "functions": 25,
        "lines": 31,
        "statements": 31
      }
    }
  }
}
